# mount     fstype  device                  [format option]   [mount options]

/cache      ext4    /dev/block/stl11        rfs                rw,nosuid,nodev,check=no
/data       ext4    /dev/block/stl10        rfs                rw,nosuid,nodev,check=no
/sdcard     vfat    /dev/block/mmcblk0p1 
/system     ext4    /dev/block/stl9         rfs                check=no
